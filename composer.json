{
    "name": "salah/laravel-custom-fields",
    "description": "Professional, flexible, and headless-ready Custom Fields (EAV) package for Laravel.",
    "keywords": [
        "Custom Fields Lab",
        "laravel",
        "laravel-custom-fields"
    ],
    "homepage": "https://github.com/salaheldeen911/laravel-custom-fields",
    "license": "MIT",
    "authors": [
        {
            "name": "Salah",
            "email": "salah.eldeen.mail@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "propaganistas/laravel-phone": "^5.0",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^2.0||^3.0",
        "pestphp/pest-plugin-arch": "^2.5||^3.0",
        "pestphp/pest-plugin-laravel": "^2.0||^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1||^2.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "CustomFields\\LaravelCustomFields\\": "src/",
            "CustomFields\\LaravelCustomFields\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CustomFields\\LaravelCustomFields\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CustomFields\\LaravelCustomFields\\LaravelCustomFieldsServiceProvider"
            ],
            "aliases": {
                "LaravelCustomFields": "CustomFields\\LaravelCustomFields\\Facades\\LaravelCustomFields"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
